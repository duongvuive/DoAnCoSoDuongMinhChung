@model WebBanHangOnline.Models.ProfileViewModel

@{
    ViewBag.Title = "ProfileKH";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ProfileKH</h2>

<div class="row">
    <div class="col-md-4">
        <h4>User Info</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>Username</dt>
            <dd>@Model.FullName</dd>

            <dt>Email</dt>
            <dd>@Model.Email</dd>

    

            <dt>Phone Number</dt>
            <dd>@Model.PhoneNumber</dd>
        </dl>
    </div>
</div>